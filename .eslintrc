{
  "rules": {
    "no-unused-vars": 0,
    "indent": [ 2, 2, {"SwitchCase": 1} ],
    "quotes": [ 2, "single" ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 2, "never" ],
    "comma-dangle": [ 2, "always-multiline" ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "google": false
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "plugins": [
    "react"
  ]
}
